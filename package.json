{
  "name": "@dx-cli-toolbox/sfdx-toolbox-package-utils",
  "description": "Utilities to better manage SalesforceDX Packages",
  "version": "0.3.0",
  "author": "John M. Daniel @ImJohnMDaniel",
  "bugs": "https://github.com/ImJohnMDaniel/sfdx-toolbox-package-utils/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.3",
    "@rauschma/stringio": "^1.4.0",
    "@salesforce/command": "^3.0.3",
    "@salesforce/core": "^2.11.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "intercept-stdout": "^0.1.2",
    "salesforce-alm": "^49.11.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.7",
    "@salesforce/dev-config": "^2.0.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.2",
    "chai": "^4.2.0",
    "globby": "^11.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ImJohnMDaniel/sfdx-toolbox-package-utils",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "toolbox": {
        "description": "Commands focused on management of SalesforceDX projects"
      },
      "toolbox:package": {
        "description": "Commands focused on management of SalesforceDX package dependencies"
      },
      "toolbox:package:dependencies": {
        "description": "Commands focused on management of SalesforceDX package dependencies"
      }
    },
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ImJohnMDaniel/sfdx-toolbox-package-utils",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
